package part3.ex3.Getter¿ÍSetters;

public class Exam {
	int kor;
	int eng;
	int math;
	
	public int getKor() {
		return kor;
	}
	
	public void setKor(int kor) {
		this.kor = kor;
	}
	
	public int getEng() {
		return eng;
	}
	
	public void setEng(int eng) {
		this.eng = eng;
	}
	
	public int getMath() {
		return math;
	}
	
	public void setMath(int math) {
		this.math = math;
	}
	
	public int total() {
		return kor + eng + math;;
	}
	
	public float avg() {
		return this.total()/3.0f;
	}
	
}
